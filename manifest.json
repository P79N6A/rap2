{
    "name": "Proxy",
    "manifest_version": 2,
    "version": "0.1.2",
    "description": "代理接口",
    "browser_action": {
      "default_icon": "./icon.png",
      "default_title": "Proxy",
      "default_popup": "dist/devtoolsPages.html"
    },
    "options_page": "options.html",
    "permissions": [
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "unlimitedStorage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "icons":
    {
        "16": "icon.png",
        "48": "icon.png"
    },
    "minimum_chrome_version": "21.0.1180.57",
    "devtools_page": "devtools.html"
  }
  